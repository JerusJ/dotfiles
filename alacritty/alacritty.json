{
  "import": [
    "~/.config/alacritty/color.yml"
  ],
  "env": {
    "TERM": "xterm-256color"
  },
  "window": {
    "dimensions": {
      "columns": 160,
      "lines": 48
    },
    "padding": {
      "x": 2,
      "y": 2
    },
    "dynamic_padding": false,
    "dynamic_title": true,
    "decorations": "None"
  },
  "draw_bold_text_with_bright_colors": true,
  "scrolling": {
    "history": 10000,
    "multiplier": 3
  },
  "font": {
    "normal": {
      "family": "Fira Code",
      "style": "Semibold"
    },
    "bold": {
      "family": "Fira Code",
      "style": "Bold"
    },
    "italic": {
      "family": "Fira Code",
      "style": "Regular Italic"
    },
    "size": 16,
    "offset": {
      "x": 0,
      "y": 0
    },
    "glyph_offset": {
      "x": 0,
      "y": 0
    }
  },
  "debug": {
    "render_timer": false
  },
  "bell": {
    "animation": "EaseOutExpo",
    "duration": 0
  },
  "window.opacity": 1,
  "mouse_bindings": [
    {
      "mouse": "Middle",
      "action": "PasteSelection"
    }
  ],
  "mouse": {
    "double_click": {
      "threshold": 300
    },
    "triple_click": {
      "threshold": 300
    },
    "hide_when_typing": true,
    "hints": {
      "launcher": "open"
    }
  },
  "selection": {
    "semantic_escape_chars": ",â”‚`|:\"' ()[]{}<>",
    "save_to_clipboard": true
  },
  "cursor": {
    "style": "Block",
    "unfocused_hollow": false
  },
  "live_config_reload": true,
  "shell": {
    "program": "tmux",
    "args": [
      "new-session",
      "-A",
      "-D",
      "-s",
      "main"
    ]
  },
  "key_bindings": [
    {
      "key": "V",
      "mods": "Command",
      "action": "Paste"
    },
    {
      "key": "C",
      "mods": "Command",
      "action": "Copy"
    },
    {
      "key": "Q",
      "mods": "Command",
      "action": "Quit"
    },
    {
      "key": "N",
      "mods": "Command",
      "command": {
        "program": "open",
        "args": [
          "-nb",
          "io.alacritty"
        ]
      }
    },
    {
